<?xml version="1.0" encoding="ISO-8859-1"?>
<VXA Version="1.1">
    <GPU>
        <HeapSize>0.5</HeapSize>
    </GPU>
    <Simulator>
        <SecondaryExperiment>1</SecondaryExperiment>
        <SelfReplication>1</SelfReplication>
        <ReinitializeInitialPositionAfterThisManySeconds>0.5</ReinitializeInitialPositionAfterThisManySeconds>
        <SettleTimeBeforeNextRoundOfReplication>0.05</SettleTimeBeforeNextRoundOfReplication> <!--add this to stop condition-->
        <MaxNewVoxelsAddedMidSim>20000</MaxNewVoxelsAddedMidSim>
        <MinimumBotSize>10</MinimumBotSize>
        <MaxDistInVoxelLengthsToCountAsPair>1.5</MaxDistInVoxelLengthsToCountAsPair>
        <EnableCilia>1</EnableCilia>
        <RandomizeCiliaEvery>0.1</RandomizeCiliaEvery>
        <RecordHistory>
            <RecordStepSize>50</RecordStepSize>
            <RecordVoxel>1</RecordVoxel>
            <RecordLink>1</RecordLink>
            <RecordFixedVoxels>0</RecordFixedVoxels>
        </RecordHistory>
        <FitnessFunction>
            <mtVAR>t</mtVAR>
        </FitnessFunction>
        <AttachDetach>
            <EnableCollision>1</EnableCollision>
            <EnableDetach>1</EnableDetach>
            <EnableAttach>1</EnableAttach>
            <KeepJustOneIfManyHaveSameGroupPosition>1</KeepJustOneIfManyHaveSameGroupPosition>
            <watchDistance>1.0</watchDistance>
            <boundingRadius>0.75</boundingRadius>
            <SafetyGuard>0</SafetyGuard>
            <AttachCondition>
                <Condition_0>
                    <mtCONST>1</mtCONST>
                </Condition_0>
            </AttachCondition>
        </AttachDetach>
        <Integration>
            <DtFrac>0.9</DtFrac>
        </Integration>
        <StopCondition>
            <StopConditionFormula>
                <mtSUB>
                    <mtVAR>t</mtVAR>
                    <mtCONST>2.2</mtCONST>
                </mtSUB>
            </StopConditionFormula>
        </StopCondition>
        <Damping>
            <BondDampingZ>1</BondDampingZ>
            <ColDampingZ>0.8</ColDampingZ>
            <SlowDampingZ>0.01</SlowDampingZ>
        </Damping>
    </Simulator>
    <Environment>
        <Thermal>
            <TempPeriod>0</TempPeriod><!--never-->
        </Thermal>
        <Gravity>
            <GravEnabled>1</GravEnabled>
            <GravAcc>-2</GravAcc>
            <FloorEnabled>1</FloorEnabled>
        </Gravity>
    </Environment>
    <VXC Version="0.94">
        <Lattice>
            <Lattice_Dim>0.01</Lattice_Dim>
        </Lattice>
        <Palette>
            <Material ID="1">
                <Name>Cilia</Name>
                <Display>
                    <Red>0.44</Red>
                    <Green>0.93</Green>
                    <Blue>0.72</Blue>
                    <Alpha>0.9</Alpha>
                </Display>
                <Mechanical>
                    <isMeasured>1</isMeasured>
                    <isTarget>0</isTarget>
                    <RemoveFromSimulationAfterThisManySeconds>0.01</RemoveFromSimulationAfterThisManySeconds><!--need to redo this logic-->
                    <EndSimIfCompletelyRemoved>1</EndSimIfCompletelyRemoved>
                    <LockZ>1</LockZ>
                    <Cilia>0.9</Cilia>
                    <MatModel>0</MatModel><!--0 = no failing-->
                    <Elastic_Mod>5e+04</Elastic_Mod>
                    <Fail_Stress>0</Fail_Stress>
                    <Density>1e+03</Density>
                    <Poissons_Ratio>0.35</Poissons_Ratio>
                    <CTE>0</CTE>
                    <MaterialTempPhase>0</MaterialTempPhase>
                    <uStatic>0</uStatic>
